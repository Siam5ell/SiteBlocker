{
    "manifest_version": 3,
    "name": "Site Blocker",
    "description": "Base Level Extension",
    "version": "1.0",
    "permissions": [
        "activeTab",
        "scripting",
        "tabs"
      ],
    "icons": {
        "16": "images/icon-16.png",
        "32": "images/icon-32.png",
        "48": "images/icon-48.png",
        "128": "images/icon-128.png"
    },"background": {
        "service_worker":"background.js",
        "type": "module"
      },
    "incognito":"split"

}